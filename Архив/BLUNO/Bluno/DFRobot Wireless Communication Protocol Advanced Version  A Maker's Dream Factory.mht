=0A=
void setup() {=0A=
  mytest.init();=0A=
  Serial.begin(57600);=0A=
}=0A=
=0A=
void loop() {=0A=
  //Receive frame from serial. This function should be put in the loop =
and called periodically as following:=0A=
  if (mytest.receiveFrame()) {=0A=
    //if the received command is "speed", do the "speed" process. =0A=
    if (mytest.receivedCommand=3D=3D"speed") {=0A=
      //the "speed" process=0A=
      //the received command, the type is "Class String"=0A=
      Serial.println(mytest.receivedCommand);=0A=
      //the Lenth of received Content =0A=
      Serial.println(mytest.receivedContentLenth);=0A=
      for (int i=3D0; i&lt;mytest.receivedContentLenth; i++) {=0A=
        //the received content in an array=0A=
        Serial.println(mytest.receivedContent[i]);=0A=
      }=0A=
      Serial.println();=0A=
    }=0A=
    else if (mytest.receivedCommand=3D=3D"torque"){=0A=
      //torque process=0A=
      //the same as above=0A=
      Serial.println(mytest.receivedCommand);=0A=
      Serial.println(mytest.receivedContentLenth);=0A=
      for (int i=3D0; i&lt;mytest.receivedContentLenth; i++) {=0A=
        Serial.println(mytest.receivedContent[i]);=0A=
      }=0A=
      Serial.println();=0A=
    }=0A=
    else{=0A=
      //no matching command=0A=
      Serial.println("command not available");=0A=
    }=0A=
  }=0A=
}</TEXTAREA></DIV>
